{
  "name": "shelf-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "swagger:convert": "swagger2openapi ../docs/swagger/swagger.json -o ../docs/swagger/openapi.json",
    "generate:types": "bun run swagger:convert && openapi-typescript ../docs/swagger/openapi.json -o src/api/types.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-router": "^1.92.0",
    "openapi-fetch": "^0.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "^1.92.0",
    "@tanstack/router-plugin": "^1.92.0",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.13.0",
    "jsdom": "^25.0.0",
    "openapi-typescript": "^7.4.0",
    "postcss": "^8.4.49",
    "swagger2openapi": "^7.0.8",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.3",
    "vitest": "^2.1.0"
  }
}
